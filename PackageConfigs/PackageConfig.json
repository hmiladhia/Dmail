{
  "__name": "DmailPackageConfig",
  "name": "Dmail",
  "author": "Dhia Hmila",
  "author_email": "hmiladhia@hotmail.fr",
  "description": "This is a simple package that provides a quick way to send emails through code.",
  "long_description": "${read_file[README.md]}",
  "long_description_content_type": "text/markdown",
  "url": "https://github.com/hmiladhia/Dmail",
  "packages": ["Dmail", "Dmail.esp", "Dmail.mixin", "Dmail.api"],
  "keywords": "email mail markdown md html style css",
  "install_requires": ["markdown", "html2text", "premailer", "Pygments"],
  "extras_require": {
    "GmailApi":  ["google-api-python-client", "google-auth-httplib2", "google-auth-oauthlib"]
  },
  "classifiers": [ 
    "Programming Language :: Python :: 3", 
    "License :: OSI Approved :: MIT License", 
    "Operating System :: OS Independent" 
  ],
  "python_requires": ">= 3.6"
}
